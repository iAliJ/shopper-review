
<h1>Malls Details </h1>

<table>
    <tr>
        <th>Name of mall</th>
        <th>Rating</th>
        <th>Location</th>
        <th>Opening Hours</th>
        <th>Contact</th>
        <th>Created At</th>
        <th>Edit mall</th>
        <th>Delete mall</th>
        
    </tr> 
    <% malls.forEach(function (mall) { %> 
       <tr>
        <td>
           <a href="/mall/detail?id=<%= mall._id %>">
           <%= mall.nameOfMall %>
         </a></td>
       
        
        <td> <%= mall.rating %> </td>
        <td> <%= mall.location %> </td>
        <td> <%= mall.openingHour%> </td>
        <td> <%= mall.PhoneNumber%> </td>
        <td> <%= mall.createdAt %> </td>
    
        
        <td><a href="/mall/edit?id=<%= mall._id%>">Edit Mall</a></td>
        <td><a href="/mall/delete?id=<%= mall._id%>">Delete Mall</a></td>
    </tr>
        
        <%});%>
</table>
