<h1>Add Mall</h1>

<form method="POST" action="/mall/add" >

    <div>
        <label> Name of Mall </label>
        <input type="text" name="nameOfMall" />
    </div>

    <div>
        <label> Rating </label>
        <input type="number" name="rating"/>
    </div>
    <div>
        <label>Location</label>
        <input type="String" name="location"/>
    </div>
    <div>
        <label>Opening Hours</label>
        <input type="String" name="openingHour"/>
    </div>

    <div>
        <label>Contact Information</label>
        <input type="String" name="PhoneNumber"/>
    </div>


    <div>
        <button type="submit">Create Mall</button>
    </div>
</form>